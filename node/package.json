{
  "name": "lac-server",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "prettier": {
    "trailingComma": "es5",
    "singleQuote": true,
    "printWidth": 100,
    "arrowParens": "always"
  },
  "scripts": {
    "------# deploy databse #------": "",
    "mig": "node -r source-map-support/register dist/deploy/migrateRdb.js",
    "------# databse #------": "",
    "db-create": "db-migrate --config config/database.create.json --env default db:create",
    "db-drop": "db-migrate --config config/database.create.json --env default db:drop",
    "mig-create": "node -r source-map-support/register dist/migrate/createMigration.js",
    "mig-up": "node -r source-map-support/register dist/deploy/migrateRdb.js --env user --target table",
    "mig-down": "node -r source-map-support/register dist/deploy/migrateRdb.js --env user --target table --direction down",
    "sp-apply": "node -r source-map-support/register dist/deploy/migrateRdb.js --env user --target procedure",
    "------# common #------": "",
    "build": "tsc -p tsconfig.json",
    "clean": "rimraf dist",
    "watch": "tsc -w",
    "------# testing #------": "",
    "start:bot": "node -r source-map-support/register dist/botClientApp/botClientApp.js"
  },
  "dependencies": {
    "@sentry/node": "^5.24.2",
    "ajv": "^6.10.0",
    "amqplib": "^0.5.2",
    "async": "^3.1.0",
    "axios": "^0.19.2",
    "behaviortree": "^2.0.3",
    "bluebird": "^3.7.1",
    "config": "^3.2.4",
    "db-migrate": "^0.11.3",
    "db-migrate-mysql": "^1.1.10",
    "elastic-apm-node": "^3.1.0",
    "event-dispatch": "^0.4.1",
    "express": "^4.16.2",
    "express-async-errors": "^3.1.1",
    "express-ipfilter": "^1.1.2",
    "fifo": "^2.3.0",
    "generic-pool": "^3.2.0",
    "gl-matrix": "^3.0.0",
    "graphlib": "^2.1.5",
    "heapdump": "^0.3.15",
    "ioredis": "^4.14.1",
    "json5": "^2.1.1",
    "knex": "^0.20.1",
    "lodash": "^4.17.11",
    "minimist": "^1.2.0",
    "mkdirp": "^0.5.1",
    "moment": "^2.19.1",
    "mongoose": "^5.11.13",
    "morgan": "^1.9.0",
    "node-schedule": "^1.3.0",
    "object-hash": "^2.0.0",
    "promise-mysql": "^4.1.1",
    "qs": "^6.9.1",
    "queue-fifo": "^0.2.6",
    "ramda": "^0.26.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.2.2",
    "shortid": "^2.2.8",
    "smart-buffer": "^4.1.0",
    "source-map-support": "^0.5.4",
    "stoppable": "^1.1.0",
    "typedi": "^0.8.0",
    "url-join": "^4.0.1",
    "winston": "^3.2.1",
    "winston-daily-rotate-file": "^4.1.0",
    "yn": "^3.1.1",
    "zlib": "^1.0.5"
  },
  "devDependencies": {
    "@types/amqplib": "^0.5.13",
    "@types/async": "^3.0.3",
    "@types/config": "^0.0.36",
    "@types/express": "^4.17.2",
    "@types/generic-pool": "^3.1.9",
    "@types/gl-matrix": "^2.4.5",
    "@types/graphlib": "^2.1.5",
    "@types/heapdump": "^0.3.1",
    "@types/ioredis": "^4.0.19",
    "@types/json5": "^0.0.30",
    "@types/lodash": "^4.14.147",
    "@types/minimist": "^1.2.0",
    "@types/mkdirp": "^0.5.2",
