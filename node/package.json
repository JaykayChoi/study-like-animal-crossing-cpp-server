    "@types/node-schedule": "^1.2.4",
    "@types/object-hash": "^1.3.0",
    "@types/qs": "^6.9.0",
    "@types/ramda": "^0.26.34",
    "@types/shortid": "^0.0.29",
    "@types/triple-beam": "^1.3.0",
    "@types/url-join": "^4.0.0",
    "nodemon": "^1.19.4",
    "rewire": "^4.0.1",
    "rimraf": "^3.0.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.7.2"
  }
}
